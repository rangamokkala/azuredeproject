{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ItemId as string,",
				"          VartId as string,",
				"          ItemName as string,",
				"          VartName as string,",
				"          WhNo as string,",
				"          Zone as string,",
				"          Bin as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(ItemId,",
				"          ItemName),",
				"     VartName = collect(VartName)) ~> aggregate1",
				"aggregate1 derive(VartName = toString(VartName)) ~> derivedColumn1"
			]
		}
	}
}